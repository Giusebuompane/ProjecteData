    "\n",
    "# Calcular la disponibilidad y la ocupación\n",
    "tasa_por_ciudad[\"dias_totales\"] = 30 * tasa_por_ciudad[\"total_filas\"]\n",
    "tasa_por_ciudad[\"tasa_ocupacion\"] = (\n",
